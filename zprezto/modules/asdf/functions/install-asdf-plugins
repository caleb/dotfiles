local url

local -A plugins
plugins[ruby]="https://github.com/asdf-vm/asdf-ruby.git"
plugins[elm]="https://github.com/vic/asdf-elm.git"
plugins[elixir]="https://github.com/asdf-vm/asdf-elixir.git"
plugins[erlang]="https://github.com/asdf-vm/asdf-erlang.git"
plugins[golang]="https://github.com/kennyp/asdf-golang.git"
plugins[php]="https://github.com/odarriba/asdf-php.git"
plugins[rust]="https://github.com/code-lever/asdf-rust.git"
plugins[rebar]="https://github.com/Stratus3D/asdf-rebar.git"
plugins[crystal]="https://github.com/marciogm/asdf-crystal.git"
plugins[nodejs]="https://github.com/asdf-vm/asdf-nodejs.git"
plugins[racket]="https://github.com/vic/asdf-racket.git"
plugins[clojure]="https://github.com/vic/asdf-clojure.git"
plugins[haskell]="https://github.com/vic/asdf-haskell.git"
plugins[ocaml]="https://github.com/vic/asdf-ocaml.git"
plugins[swift]="https://github.com/fcrespo82/asdf-swift.git"
plugins[scala]="https://github.com/mtatheonly/asdf-scala.git"
plugins[python]="https://github.com/tuvistavie/asdf-python.git"

for i in "${(@k)plugins}"; do
  url="${plugins[$i]}"
  asdf plugin-add $i "${url}"
done
