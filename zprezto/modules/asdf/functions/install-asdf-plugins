local url

local -A plugins
plugins[ruby]=""
plugins[elm]=""
plugins[elixir]=""
plugins[erlang]=""
plugins[golang]=""
plugins[php]=""
plugins[rust]=""
plugins[rebar]=""
plugins[crystal]=""
plugins[nodejs]=""
plugins[racket]=""
plugins[clojure]=""
plugins[haskell]=""
plugins[ocaml]=""
plugins[swift]=""
plugins[scala]=""
plugins[python]=""

for i in "${(@k)plugins}"; do
  url="${plugins[$i]}"
  if [[ -z "${url}" ]]; then
    asdf plugin-add $i
  else
    asdf plugin-add $i "${url}"
  fi
done
