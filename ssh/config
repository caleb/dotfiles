# Settings for all hosts
PubkeyAcceptedKeyTypes=+ssh-dss
KexAlgorithms +diffie-hellman-group1-sha1
AddKeysToAgent yes

Host *.dev
  StrictHostKeyChecking no

#
# Local machines
#
Host *.home.land.fm
  ForwardAgent yes

Host massive
  ForwardAgent yes

Host gitea
  ForwardAgent yes

Host development-vm.local
  User caleb
  HostName development-vm.local
  ForwardAgent yes

#
# Clients
#
Host tools.mojoyourfranchise.com
  User root
  HostName tools.mojoyourfranchise.com
  ForwardAgent yes

Host bdccreditreporter.com
  User root
  HostName bdccreditreporter.com
  ForwardAgent yes
